            autocomplete="secondaryReceptacleAmps"
            type="text"
            id="secondaryReceptacleAmps"
            name="contact[secondaryReceptacleAmps]"
            value="{{ form.secondaryReceptacleVoltage }}"
            placeholder="secondary receptacle"
          >
        </div>

        <div class="field">
          <label class="dropdown" for="inputType"> Style</label>
          <select
            class="dropdown"
            name="contact[secondaryReceptacleStyle]"
            id="inputType"
          >
            <option value="" selected="true">Select Receptacle Style</option>
            <option value="Single">Single</option>
            <option value="Duplex">Duplex</option>
            <option value="Twistlock">Twistlock</option>
            <option value="Pin &amp; Sleeve">Pin &amp; Sleeve</option>
          </select>
        </div>
      </div>
    </fieldset>

    <fieldset>
      <h1>Additional Options</h1>
      <div class="">
        <div>
          <h3>Choose as many of the options as required</h3>
          <div class="check-box-wrap">
            <label class="checkmark-container" for="wheels"
              ><span>Wheels</span>
