<div class="contact page-width ">
  {%- form 'contact', id: 'ContactForm', class: contact_form_class -%}
    {%- if form.posted_successfully? -%}
      <h2 class="form-status form-status-list form__message" tabindex="-1" autofocus>
        {% render 'icon-success' %}
        {{ 'templates.contact.form.post_success' | t }}
      </h2>
    {%- elsif form.errors -%}
      <div class="form__message">
        <h2 class="form-status caption-large text-body" role="alert" tabindex="-1" autofocus>
          {% render 'icon-error' %}
          {{ 'templates.contact.form.error_heading' | t }}
        </h2>
      </div>
      <ul class="form-status-list caption-large" role="list">
        <li>
          <a href="#ContactForm-email" class="link">
            {{ form.errors.translated_fields.email | capitalize }}
            {{ form.errors.messages.email }}
          </a>
        </li>
      </ul>
    {%- endif -%}
    <input
      type="hidden"
      name="contact[powerDistributionCenterEstimateForm]"
      value="Please contact the customer regarding a Power Distribution Center based on the following details."
    >
    <fieldset class="power-distribution-form">
      <h2>Please tell us about your temporary power distribution requirements.</h2>
      <h1>Portable Power Distribution Center Request Form</h1>
      <h3>
        To request an estimate, fill out as much of the form as possible or call us directly at 864.757.5248 for
        assistance.
      </h3>
    </fieldset>
    <fieldset>
      <h1>Contact</h1>
      <div class="contact__fields">
        <div class="field">
          <input
            class="field__input"
            autocomplete="name"
            type="text"
            id="name"
            name="contact[name]"
            value="{{ form.name }}"
            placeholder="Name"
          >

          <label class="field__label" for="name"> Your Name</label>
        </div>
        <div class="field">
          <input
            autocomplete="email"
            type="email"
            id="email"
            class="field__input"
            name="contact[email]"
            placeholder="Email"
            value="{{ form.email }}"
            aria-required="true"
            {% if form.errors contains 'email' %}
              aria-invalid="true"
              aria-describedby="ContactForm-email-error"
            {% endif %}
          >
          <label class="field__label" for="email"> <span aria-hidden="true">*</span>Email</label>
          {%- if form.errors contains 'email' -%}
            <small class="contact__field-error" id="ContactForm-email-error">
              <span class="visually-hidden">{{ 'accessibility.error' | t }}</span>
              <span class="form__message">
                {%- render 'icon-error' -%}
                {{- form.errors.translated_fields.email | capitalize }}
                {{ form.errors.messages.email -}}
              </span>
            </small>
          {%- endif -%}
        </div>
        <div class="field">
          <input
            class="field__input"
            autocomplete="Company Name"
            type="text"
            id="company"
            name="contact[company]"
            value="{{ form.company }}"
            placeholder="Company Name"
          >
          <label class="field__label" for="company">Business Name</label>
        </div>
        <div class="field">
          <input
            type="tel"
            id="phone"
            class="field__input"
            autocomplete="tel"
            name="contact[phone]"
            pattern="[0-9\-]*"
            value="{{ form.phone }}"
            placeholder="Business Phone"
          >
          <label class="field__label" for="phone">Phone</label>
        </div>
      </div>
    </fieldset>
    <fieldset>
      <h1>Primary Power Feed</h1>

      <div class="input-row-wrap">
        <div class="field">
          <label class="dropdown" for="inputConnector">Type</label>

          <select
            class="dropdown"
            name="contact[inputConnector]"
            id="inputConnector"
          >
            <option value=" " selected="true">select input type</option>
            <option value="Main Lug Only (MLO)">Main Lug Only (MLO)</option>
            <option value="Fused Disconnect">Fused Disconnect</option>
            <option value="Main Circuit Breaker">Main Circuit Breaker</option>
            <option value="Cam-Lok Input">Cam-Lok Input</option>
